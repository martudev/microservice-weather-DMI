{
    "name": "microservice-weather-dmi",
    "version": "1.0.0",
    "description": "A microservice to know if the temperature of a city is greater or less than 15 degrees",
    "main": "dist/index.js",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && babel src -d dist",
        "lint": "eslint \"src/**/*.js\" --ignore-pattern node_modules/",
        "lint:fix": "eslint \"**/*.js\" --ignore-pattern node_modules/ --fix",
        "start": "npm run lint && npm run build && node dist/index.js",
        "prepare": "husky install",
        "commit": "npx cz",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "microservice",
        "weather",
        "DMI",
        "OpenWeather"
    ],
    "author": "Martina Amengual",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.24.0",
        "core-js": "^3.19.1",
        "dotenv": "^10.0.0",
        "fastify": "^3.24.0",
        "qs": "^6.10.1",
        "regenerator-runtime": "^0.13.9",
        "rimraf": "^3.0.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/eslint-parser": "^7.16.3",
        "@babel/preset-env": "^7.16.0",
        "@commitlint/cli": "^14.1.0",
        "@commitlint/config-conventional": "^14.1.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "prettier": "2.4.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
